{
  "name": "grpc-examples",
  "version": "0.1.0",
  "dependencies": {
    "@grpc/grpc-js": "^0.7.9",
    "@grpc/proto-loader": "^0.1.0",
    "async": "^1.5.2",
    "google-protobuf": "^3.0.0",
    "grpc": "^1.12.0",
    "grpc-tools": "^1.8.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.6.1",
    "minimist": "^1.2.0",
    "ts-proto": "^1.18.2",
    "ts-protoc-gen": "^0.12.0"
  },
  "scripts": {
    "dev": "node main.js",
    "start": "node main.js",
    "clean": "rm -rf ./src/proto_grpc/*.proto ./src/proto_js/*.proto",
    "gen": "grpc_tools_node_protoc  -I=./src/proto/ --js_out=import_style=commonjs,binary:./src/proto_js --grpc_out=./src/proto_grpc/ --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` ./src/proto/*.proto",
    "test-cli": "node ",
    "test": "npx mocha",
    "genTs": "grpc_tools_node_protoc -I=./src/proto/ --plugin=protoc-gen-ts='./node_modules/.bin/protoc-gen-ts' --js_out=import_style=commonjs,binary:./src/proto_ts/generate --ts_out=./src/proto_ts/ --grpc_out=./src/proto_grpc_ts/ ./src/proto/*.proto",
    "genTsa": "grpc_tools_node_protoc -I=./src/proto/ --plugin=protoc-gen-ts='./node_modules/.bin/protoc-gen-ts_proto' --ts_proto_out=./src/proto_ts/ --grpc_out=./src/proto_grpc_ts/ ./src/proto/*.proto"

  },
  "devDependencies": {
    "mocha": "^7.1.1"
  },
  "imports": {
    "@Gm/": [
      "./src/"
    ],
    "@npm": [
      "./node_module/."
    ],
    "@std/": [
      "https://deno.land/std/"
    ]
  }
}
