{
  "name": "unlight-zwei-scptrunner",
  "version": "1.0.0",
  "description": "Unlight Zwei - Script Interpreter Runner",
  "main": "index.js",
  "scripts": {
    "test": "deno run .dpkg/runcmd/test.ts --importmap=package.json",
    "test_redis": "deno run .dpkg/runcmd/test_redis.ts --importmap=package.json",
